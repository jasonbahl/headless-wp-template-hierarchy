    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "clsx": "^1.1.1",
    "cross-fetch": "^3.1.5",
    "deepmerge": "^4.2.2",
    "graphql": "^16.3.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.4.0",
